    "require": {
        "php": "^8.1",
        "cakephp/cakephp": "4.5",
        "cakephp/migrations": "^4.5",
        "cakephp/datasource": "^5.1",
        "cakephp/database": "^5.1",
        "cakephp/core": "^5.1"
    },
    "require-dev": {
        "cakephp/bake": "^2.6",
        "cakephp/cakephp-codesniffer": "^4.5",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "~8.5.0 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Tyrellsys\\Itemmanager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "MattTyrell",
            "email": "matthew@tyrellsys.com.my"
        }
